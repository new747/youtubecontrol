{
  "name": "youtube control",
  "version": "2.0.0",
  "manifest_version": 3,
  "host_permissions": [
    "https://script.google.com/"
  ],
  "description": "youtube control",
  "content_scripts": [{
    "matches": ["https://www.youtube.com/*"],
    "js": [
      "content.js",
      "options.js"
    ]
  }]
}